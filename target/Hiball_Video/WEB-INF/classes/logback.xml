<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%d{yyyy/MM/dd HH:mm:ss.SSS}] &lt;Hiball_Video&gt; %-5level %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>
	
	<logger name="org.springframework.context" level="DEBUG"></logger>
	<logger name="org.springframework.web.servlet.view.json" level="DEBUG"></logger>
	<logger name="org.springframework.data.mybatis" level="ERROR"></logger>
	
	<root level="DEBUG">
		<appender-ref ref="STDOUT"/>
	</root>
	
</configuration>